[Unit]
Description={{ cookiecutter.systemd_service_description }}

[Service]
Type=dbus
BusName={{ cookiecutter.dbus_bus_name }}
ExecStart=/usr/bin/{{ cookiecutter.service_executable }}
EnvironmentFile=-/etc/default/gwn
EnvironmentFile=-/etc/default/{{ cookiecutter.debian_package_name }}

[Install]
WantedBy=xorg.target
